{
  "name": "raiden-swagger-sdk",
  "version": "1.0.0",
  "description": "Swagger generated sdk",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "generate": "TS_POST_PROCESS_FILE=\"prettier --write\" openapi-generator generate -g typescript-rxjs -i ./swagger.yaml -o ./ --enable-post-process-file"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdgvhbh/raiden-swagger-sdk.git"
  },
  "keywords": [
    "payments",
    "raiden"
  ],
  "author": "Ryan Lee (drdgvhbh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drdgvhbh/raiden-swagger-sdk/issues"
  },
  "dependencies": {
    "rxjs": "^6.5.2"
  }
}
